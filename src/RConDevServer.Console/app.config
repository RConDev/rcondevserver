<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>

    <!--<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />-->

    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="RConDevServer.Console.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
      <section name="RConDevServer.Protocol.Dice.Battlefield3.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=4.3.1.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
  </configSections>
  <applicationSettings>
    <RConDevServer.Console.Properties.Settings>
      <setting name="ListenOnPort" serializeAs="String">
        <value>11000</value>
      </setting>
      <setting name="MaxSessions" serializeAs="String">
        <value>2</value>
      </setting>
    </RConDevServer.Console.Properties.Settings>
    <RConDevServer.Protocol.Dice.Battlefield3.Properties.Settings>
      <setting name="ReservedSlotsDataFile" serializeAs="String">
        <value>bf3-reservedslots.cfg</value>
      </setting>
    </RConDevServer.Protocol.Dice.Battlefield3.Properties.Settings>
  </applicationSettings>
  <connectionStrings>
    <add name="Default" connectionString="Data Source=rcondevdata.sdb;" />
  </connectionStrings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
